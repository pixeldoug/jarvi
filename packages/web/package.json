{
  "name": "@jarvi/web",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "storybook": "STORYBOOK_DISABLE_TELEMETRY=1 storybook dev -p 6006 --ci",
    "build-storybook": "storybook build",
    "tokens:import": "node src/design-system/scripts/import-from-figma.js",
    "tokens:generate": "npm run tokens:import && node src/design-system/scripts/generate-tokens.js",
    "tokens:watch": "nodemon --watch src/design-system/tokens/figma --ext json --exec \"npm run tokens:generate\""
  },
